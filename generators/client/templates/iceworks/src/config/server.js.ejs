import {env} from './app';

// eslint-disable-next-line import/no-mutable-exports
let serverConfig;

switch (env) {
  case 'uat':
    serverConfig = {
      domain: '',
      apiEndpoint: '<%= uatEndpoint %>',
    };
    break;
  case 'pro':
    serverConfig = {
      domain: '',
      apiEndpoint: '<%= proEndpoint %>',
    };
    break;
  default:
    serverConfig = {
      domain: 'http://localhost:3000',
      apiEndpoint: '//localhost:8080',
    };
    break;
}

export default serverConfig;
